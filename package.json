{
  "name": "nwalk",
  "version": "0.1.0",
  "description": "local are network map creation using walk sequencing",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "randomcolor": "^0.5.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.1.0",
    "three": "^0.93.0"
  },
  "build": {
    "appId": "nwalk",
    "mac": {
      "category": "opengl.network.map"
    }
  },
  "main": "src/electron.js",
  "scripts": {

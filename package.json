    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "eslint-watch": "^3.1.5",
    "foreman": "^3.0.0",
    "jester": "^0.2.3",
    "parcel-bundler": "^1.9.0",
